<Application
    x:Class="Personate.UWP.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Personate.UWP"
    xmlns:view="using:Personate.UWP.MVVM.View"
    xmlns:viewmodel="using:Personate.UWP.MVVM.ViewModel">
    <Application.Resources>
        

        <DataTemplate
            x:Key="WallCardTemplate"
            x:DataType="viewmodel:WallCardViewModel">
            <view:WallCardView/>
        </DataTemplate>

        <DataTemplate
            x:Key="WallTemplate"
            x:DataType="viewmodel:WallViewModel">
            <view:WallView/>
        </DataTemplate>
    </Application.Resources>
</Application>

